{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [".*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
